                                        let num2 = Number($("#fbnum2").val());


                                        let output = "";


                                                    for (var loop = 1; loop <= 100; loop++) {

                                                        if (loop % num1 == 0 && loop % num2 == 0) {

                                                            output += "FizzBuzz, ";
                                                        }
                                                        else if (loop % num1 == 0) {

                                                            output += "Fizz, ";
                                                        }
                                                        else if (loop % num2 == 0) {

                                                            output += "Buzz, ";
                                                        }
                                                        else {

                                                            output += loop + ", ";
                                                        }
                                                    }


                                                    $("#fbout").text(output.substring(0, output.length - 2));
                                                });

                                                $("#btnclose").click(function () {

                                                    $("#fbnum1").val("");
                                                    $("#fbnum2").val("");
                                                    $("#fbout").text("");
                                                });
